{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "aaxion-client",
  "version": "0.1.0",
  "identifier": "com.codershubinc.aaxion",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000/d",
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Aaxion Drive",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}